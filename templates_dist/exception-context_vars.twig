<h2>Context variables</h2><div>{% for var in exception.getContext %}{% set showFull = loop.index0 < 10 %}{% set varId = 'context-variable-' ~ loop.index %}<div class="card"><div class="card-header"><a data-toggle="collapse" class="var-header" data-target="#{{ varId }}">{{ var.getName }}</a></div><div id="{{ varId }}" class="collapse max-height{{ showFull ? ' show' }}"><div class="card-body"><pre>{{ var.dumpAsString }}</pre></div></div></div>{% endfor %}</div>
